{
  "expo": {
    "name": "CoraMed",
    "slug": "CoraMed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "coramed",
    "splash": {
      "image": "./src/assets/images/Logo.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coramed.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "coramed"
            ]
          }
        ]
      },
      "associatedDomains": [
        "applinks:coramed.com"
      ]
    },
    "android": {
      "package": "com.coramed.app",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "coramed.com",
            "pathPrefix": "/reset-password"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "coramed"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "12fe81c5-5bf2-4739-896d-95174f570c3a"
      }
    },
    "owner": "santi_ote"
  }
}
